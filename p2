//Write a program to swap the values of variable a & b.
#include <stdio.h>

int main() {
    int a, b, temp;
    printf("Enter the value of variable a: ");
    scanf("%d", &a);
    printf("Enter the value of variable b: ");
    scanf("%d", &b);
    printf("Before swapping: a = %d, b = %d\n", a, b);
    temp = a;
    a = b;
    b = temp;
    printf("After swapping: a = %d, b = %d\n", a, b);

    return 0;
}
